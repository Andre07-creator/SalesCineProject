@model IEnumerable<SalesCineProject.Models.Ticket>

@{
    ViewData["Title"] = "SaleTickets";
}
<h2>@ViewData["Title"]</h2>

<table class="table table-striped table-hover">

    <thead>
        <tr class="success">
            <th>
                @Html.DisplayNameFor(model => model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NameMovie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Hour)
            </th>
            <th>
                @Html.DisplayNameFor(model => model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model)
            </th>
            <th>
                @Html.DisplayNameFor(model => model)
            </th>
        </tr>
    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <th>
                    @Html.DisplayFor(modelItem => item)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.NameMovie)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item.Hour)
                </th>
                <th>
                    @Html.DisplayFor(modelitem => item)
                </th>
                <th>
                    @Html.DisplayFor(modelItem => item)
                </th>

            </tr>
        }
    </tbody>

</table>
